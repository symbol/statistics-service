{
    "name": "symbol-statistics-service",
    "version": "1.1.2",
    "description": "",
    "scripts": {
        "dev": "nodemon",
        "build": "shx rm -rf ./dist/ && tsc && shx cp -r openapi dist/openapi",
        "build:clients": "bash scripts/build-clients.sh",
        "start": "node dist/app.js",
        "version": "echo $npm_package_version",
        "prettier": "prettier --write ./src",
        "lint": "eslint --cache src/ --ext .ts",
        "lint:fix": "eslint src/ --ext .ts --fix",
        "style:fix": "npm run prettier && npm run lint:fix",
        "test:openapi": "swagger-cli validate openapi/openapi.yml"
    },
    "main": "dist/app.js",
    "typings": "dist/index.d.ts",
    "_moduleAliases": {
        "@src": "dist"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.2.6",
        "@types/lodash": "^4.14.176",
        "@types/node": "^14.14.10",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^4.4.0",
        "@typescript-eslint/parser": "^4.4.0",
        "eslint": "^7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.3",
        "openapi-to-postmanv2": "^2.6.0",
        "prettier": "^2.1.2",
        "shx": "^0.3.3",
        "swagger-cli": "^4.0.4",
        "ts-node": "^8.10.1",
        "tslint": "^6.1.2",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/tcp-ping": "^0.1.1",
        "@types/uuid": "^7.0.3",
        "axios": "^0.21.2",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "lodash": "^4.17.21",
        "module-alias": "^2.2.2",
        "mongoose": "^5.11.12",
        "symbol-sdk": "^1.0.1",
        "tcp-ping": "^0.1.1",
        "utf8": "^3.0.0",
        "uuid": "^8.0.0",
        "winston": "^3.3.3"
    }
