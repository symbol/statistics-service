        "@types/express": "^4.17.6",
        "@types/tcp-ping": "^0.1.1",
        "@types/uuid": "^7.0.3",
        "axios": "^0.21.2",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "humanize-duration": "^3.27.2",
        "module-alias": "^2.2.2",
        "mongoose": "^5.9.16",
        "symbol-sdk": "^2.0.0",
        "tcp-ping": "^0.1.1",
        "utf8": "^3.0.0",
        "uuid": "^8.0.0",
        "winston": "^3.3.3"
    }
